{
  "name": "node-s3tables",
  "version": "0.0.2",
  "description": "node api for dealing with s3tables",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "ts-check": "tsc --noEmit",
    "lint": "eslint src test",
    "pretty": "prettier -u --write \"**/*\" --log-level warn",
    "test": "tsx --test test/*.test.ts",
    "test:cover": "tsx --test --experimental-test-coverage test/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jim-lake/node-mysql-cron.git"
  },
  "author": {
    "name": "Jim Lake"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@aws-sdk/client-s3tables": "^3.899.0",
    "avro-js": "^1.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.6.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "rollup": "^4.52.3",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  }
}
